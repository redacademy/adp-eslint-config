{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "max-len": 0,
      "global-require": 0,
      "no-case-declarations": 0,
      "no-param-reassign": 0,
      "react/prefer-stateless-function": 0,
      "react/jsx-no-bind": 0
    }
 }
